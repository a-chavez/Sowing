<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorGreenDark"
    tools:context=".CalendarFragment">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="45dp"
        android:layout_height="match_parent"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layouTitleinter" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/block_02"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/block_01"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/layouTitleCalendar"
        app:tint="@color/colorGreenDark" />

    <include
        android:id="@+id/layouTitleCalendar"
        layout="@layout/block_sup_inter" />


    <TextView
        android:id="@+id/tvAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/back_stroke"
        android:fontFamily="@font/amita"
        android:paddingStart="8sp"
        android:paddingEnd="8sp"
        android:text="Listar Todo"
        android:textColor="@color/colorLight"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/layouTitleCalendar" />

    <include
        android:id="@+id/include"
        layout="@layout/menuarroz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="@drawable/back_stroke"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/tvAll">

        <TextView
            android:id="@+id/tvEne"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Ene"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="0"
            app:layout_row="0" />

        <TextView
            android:id="@+id/tvFeb"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Feb"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="0" />

        <TextView
            android:id="@+id/tvMar"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Mar"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="2"
            app:layout_row="0" />

        <TextView
            android:id="@+id/tvAbr"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Abr"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="0"
            app:layout_row="1" />

        <TextView
            android:id="@+id/tvMay"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="May"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="1" />

        <TextView
            android:id="@+id/tvJun"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Jun"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="2"
            app:layout_row="1" />

        <TextView
            android:id="@+id/tvJul"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Jul"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="0"
            app:layout_row="2" />

        <TextView
            android:id="@+id/tvAgo"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Ago"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="2" />

        <TextView
            android:id="@+id/tvSep"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Sep"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="2"
            app:layout_row="2" />

        <TextView
            android:id="@+id/tvOct"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Oct"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="0"
            app:layout_row="3" />

        <TextView
            android:id="@+id/tvNov"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Nov"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="3" />

        <TextView
            android:id="@+id/tvDic"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:fontFamily="@font/amita"
            android:gravity="center"
            android:shadowColor="@color/colorGray"
            android:shadowRadius="12"
            android:text="Dic"
            android:textColor="@color/colorLight"
            android:textSize="32sp"
            android:textStyle="bold"
            app:layout_column="2"
            app:layout_row="3" />
    </androidx.gridlayout.widget.GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
